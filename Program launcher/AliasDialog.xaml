<Window x:Class="Program_launcher.AliasDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Program_launcher"
        mc:Ignorable="d"
        Title="AliasDialog" Height="157.103" Width="180.084">
    <StackPanel>
        <TextBlock Text="Input program alias" Margin="10" HorizontalAlignment="Center" />
        <TextBox x:Name="AliasTextBox" Margin="10" />
        <Button Content="OK" Click="Button_Click"  Margin="10" />
    </StackPanel>
</Window>
